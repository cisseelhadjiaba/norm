#!/bin/bash

depends_on libuuid
depends_on curl
depends_on libpng
depends_on libjpeg
depends_on libasound
depends_on pixman
depends_on glib
depends_on liblzo
depends_on libnss

fetch_source http://wiki.qemu-project.org/download/qemu-2.2.1.tar.bz2 4d1cefc9861fb90979497e8c0bffd223103aa240

do_unpack_compile --disable-sdl --disable-gtk --disable-vnc --disable-attr --with-system-pixman
