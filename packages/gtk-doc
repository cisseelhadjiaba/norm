#!/bin/bash

depends_on libxslt
depends_on docbook-xml
depends_on docbook-xsl
depends_on itstool
depends_on gettext

fetch_source https://download.gnome.org/sources/gtk-doc/1.26/gtk-doc-1.26.tar.xz a34d5c4730c64c164d34dd3b16ed30f6e2451b1d

## gtk-doc doesn't have any tests
do_unpack_compile_notest --with-xml-catalog="$PREFIX"/etc/xml/catalog
