#!/bin/bash

depends_on pkg-config
depends_on autoconf automake libtool
depends_on libxml
depends_on zlib
depends_on glib
depends_on libgmp
needs_tool bison
needs_tool flex

fetch_source https://gstreamer.freedesktop.org/src/gstreamer/gstreamer-1.14.0.tar.xz 6ed978dff1d04c2b65e55c9e1c7c815ecd5c0e1f

do_unpack_compile --disable-introspection
