#!/bin/bash

depends_on libgstreamer
depends_on libasound
depends_on libogg
depends_on libtheora
depends_on libvorbis
depends_on libsm
depends_on libx11
depends_on libxext
depends_on libxv
depends_on liborc
depends_on libpango

fetch_source https://gstreamer.freedesktop.org/src/gst-plugins-base/gst-plugins-base-1.14.1.tar.xz ca404fb17bd4fa553e8a6f026a0ec5d6d3506aa7

do_unpack_compile --disable-gnome_vfs --disable-cdparanoia --disable-libvisual --disable-introspection
