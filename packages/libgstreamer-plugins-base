#!/bin/bash

depends_on libgstreamer
depends_on libasound
depends_on libogg
depends_on libtheora
depends_on libvorbis
depends_on libsm
depends_on libx11
depends_on libxext
depends_on libxv
depends_on liborc
depends_on libpango

fetch_source https://gstreamer.freedesktop.org/src/gst-plugins-base/gst-plugins-base-1.14.0.tar.xz 9fcba24242252b41018f506bdd890538e6b589d4

do_unpack_compile --disable-gnome_vfs --disable-cdparanoia --disable-libvisual --disable-introspection
