#!/bin/bash

#depends_on autoconf automake libtool
depends_on libsasl
depends_on openssl
depends_on groff

fetch_source https://www.openldap.org/software/download/OpenLDAP/openldap-release/openldap-2.4.46.tgz a9ae2273eb9bdd70090dafe0d018a3132606bef6

do_unpack
#do_autoreconf
do_compile --disable-slapd

