#!/bin/bash

depends_on xz
depends_on libjpeg
depends_on zlib
depends_on libjbig

fetch_source http://download.osgeo.org/libtiff/tiff-4.0.9.tar.gz 87d4543579176cc568668617c22baceccd568296
fetch_debian http://http.debian.net/debian/pool/main/t/tiff/tiff_4.0.9-3.debian.tar.xz 602f7c1a6765bce309e3b79e48e125624a4591a9

do_undebian_compile --without-x
