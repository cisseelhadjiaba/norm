#!/bin/bash

depends_on xz ## for LZMA2-compressed tiff files
depends_on libjbig ## for ISO JBIG-compressed tiff files
depends_on libjpeg ## for JPEG-compressed tiff files
depends_on zlib ## for Deflate-compressed tiff files

fetch_source http://download.osgeo.org/libtiff/tiff-4.0.9.tar.gz 87d4543579176cc568668617c22baceccd568296
fetch_debian http://http.debian.net/debian/pool/main/t/tiff/tiff_4.0.9-6.debian.tar.xz 01cb0f32986d44622a7f5af6d596d5b4bece91f7

do_undebian_compile --without-x
