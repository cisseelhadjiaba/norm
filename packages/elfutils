#!/bin/bash

depends_on zlib  ## for zlib-compressed debug sections
needs_tool m4
depends_on xz    ## opportunistic linking, for lzma-compressed debug sections
depends_on bzip2 ## opportunistic linking, for bzip2-compressed debug sections

fetch_source https://fedorahosted.org/releases/e/l/elfutils/0.167/elfutils-0.167.tar.bz2 91cec7fec0031758cc0c842a87ee8c5faa4d8107

do_unpack_compile --program-prefix=eu-
