#!/bin/bash

depends_on pkg-config
depends_on libx11
depends_on libxext
depends_on libxi

fetch_source http://ftp.de.debian.org/debian/pool/main/libx/libxtst/libxtst_1.2.1.orig.tar.gz
fetch_debian http://ftp.de.debian.org/debian/pool/main/libx/libxtst/libxtst_1.2.1-1+deb7u1.diff.gz

do_undebian_compile
