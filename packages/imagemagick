#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2
depends_on freetype
depends_on glib
depends_on libpcre
depends_on libxml

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.7-22.tar.xz 5965e9ba6d8c69ff702d1ed3e60bdb218ad2851e

do_unpack_compile --without-x
