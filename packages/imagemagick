#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://github.com/ImageMagick/ImageMagick/archive/7.0.4-4.tar.gz 05991ab1633a1bcb22bd66ce9dd702be6da9691d

do_unpack_compile --without-x
