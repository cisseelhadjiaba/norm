#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.5-5.tar.xz c9acfbce7ecbb7dbeada7220b0b683ba67f56faf

do_unpack_compile --without-x
