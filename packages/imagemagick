#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://github.com/ImageMagick/ImageMagick/archive/7.0.4-7.tar.gz b8ea1b821eddf94d6ba174278b797a6b020ffc73

do_unpack_compile --without-x
