#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.6-10.tar.xz 3fce532d990a110cfc0a3f0c8c563ee703d112cc

do_unpack_compile --without-x
