#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.7-17.tar.xz aff2dbe04dce6d5f8f11e34f9d4b18ecb3260a55

do_unpack_compile --without-x
