#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.7-5.tar.xz bd4fa79ea11ec6fe9af21d76a3fb98b1eeaf7c2e

do_unpack_compile --without-x
