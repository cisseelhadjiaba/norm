#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://github.com/ImageMagick/ImageMagick/archive/7.0.4-5.tar.gz 41e3c459a814f0f564e94b911b1df22268999fec

do_unpack_compile --without-x
