#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.7-7.tar.xz da3a83be342b37c5b48eea2daf85d10e12dec981

do_unpack_compile --without-x
