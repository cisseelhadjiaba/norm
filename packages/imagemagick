#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.7-0.tar.xz bdccbbf397b2b9cef0f8305014b49ef6b0ffcc53

do_unpack_compile --without-x
