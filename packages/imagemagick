#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.6-9.tar.xz d385502772cc564bf266883eda8304c571470111

do_unpack_compile --without-x
