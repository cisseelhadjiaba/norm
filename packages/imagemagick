#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.5-4.tar.xz 118b2d1753cf5eb0761ea8dac068e24217b8e32b

do_unpack_compile --without-x
