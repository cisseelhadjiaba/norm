#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.5-9.tar.xz 6ca92e6714a3495eb0749c7a29a5f799484d57b7

do_unpack_compile --without-x
