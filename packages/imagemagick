#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://dl.bintray.com/homebrew/mirror/imagemagick-7.0.7-10.tar.xz 05145816194a38fe1108d87393429ae52ee7745d

do_unpack_compile --without-x
