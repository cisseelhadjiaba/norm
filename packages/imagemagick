#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://github.com/ImageMagick/ImageMagick/archive/7.0.4-2.tar.gz 6f26d190fac450dec941c726e6e652371746a422

do_unpack_compile --without-x
