#!/bin/bash

depends_on libjpeg
depends_on libjbig
depends_on libpng
depends_on libtiff
depends_on libwebp
depends_on libbpg
depends_on zlib
depends_on bzip2

fetch_source https://github.com/ImageMagick/ImageMagick/archive/7.0.4-0.tar.gz 1518a3545c6206c768a7b66a1beaa4a102b6669a

do_unpack_compile --without-x
