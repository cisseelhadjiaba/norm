#!/bin/bash

[[ $OSTYPE == *linux* ]] && depends_on libattr
depends_on python ## builds a python module, system-provided python might not have dev headers and libs
depends_on libbsd
depends_on libacl

fetch_source https://www.samba.org/ftp/talloc/talloc-2.1.12.tar.gz eed04fd021a494d2d4c2d95564fbf4bbceacd3f5

do_unpack_compile

