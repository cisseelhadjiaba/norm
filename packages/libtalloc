#!/bin/bash

[[ $OSTYPE == *linux* ]] && depends_on libattr
depends_on python ## builds a python module, system-provided python might not have dev headers and libs
depends_on libbsd
depends_on libacl

fetch_source https://www.samba.org/ftp/talloc/talloc-2.1.11.tar.gz 01b57d8f72ec85f4f2a7be9a5176dfbb3461808c

do_unpack_compile

