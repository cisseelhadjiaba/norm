#!/bin/bash

[[ $OSTYPE == *linux* ]] && depends_on libattr
depends_on python ## builds a python module, system-provided python might not have dev headers and libs
depends_on libbsd
depends_on libacl

fetch_source https://www.samba.org/ftp/talloc/talloc-2.1.10.tar.gz 4b59061f0ee5f337aacef0c258a4ffd05b3fec6c

do_unpack_compile

