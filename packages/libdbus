#!/bin/bash

depends_on pkg-config
depends_on libselinux
depends_on libexpat
depends_on libsm
depends_on libice
depends_on libx11
depends_on libxcb
depends_on libxau
depends_on libxdmcp

fetch_source http://dbus.freedesktop.org/releases/dbus/dbus-1.10.14.tar.gz ea9efcf5439b53c3bdcabf8efba838c783c3639d

do_unpack_compile --disable-modular-tests --disable-libaudit --disable-xml-docs --disable-doxygen-docs

