#!/bin/bash

fetch_source https://ftp.mozilla.org/pub/nspr/releases/v4.15/src/nspr-4.15.tar.gz 56030e0177849034ba3027a23ae2a7f8ed41f379

EXTRA_OPTIONS=
[[ $HOSTTYPE == *64* ]] && EXTRA_OPTIONS=--enable-64bit

do_unpack
SRCDIR=$SRCDIR/nspr
do_compile_notest --enable-pthreads --enable-ipv6 --enable-optimize --disable-debug $EXTRA_OPTIONS ## does not have tests
