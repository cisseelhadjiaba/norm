#!/bin/bash

fetch_source https://ftp.mozilla.org/pub/nspr/releases/v4.13.1/src/nspr-4.13.1.tar.gz ec6660257d41267a0e6ee25812e9012cb9ddd274

EXTRA_OPTIONS=
[[ $HOSTTYPE == *64* ]] && EXTRA_OPTIONS=--enable-64bit

do_unpack
SRCDIR=$SRCDIR/nspr
do_compile_notest --enable-pthreads --enable-ipv6 --enable-optimize --disable-debug $EXTRA_OPTIONS ## does not have tests
