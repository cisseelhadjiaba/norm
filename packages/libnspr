#!/bin/bash

fetch_source https://ftp.mozilla.org/pub/nspr/releases/v4.16/src/nspr-4.16.tar.gz b295ee2550b43edc0598c20663ceef9fa3fafe51

EXTRA_OPTIONS=
[[ $HOSTTYPE == *64* ]] && EXTRA_OPTIONS=--enable-64bit

do_unpack
SRCDIR=$SRCDIR/nspr
do_compile_notest --enable-pthreads --enable-ipv6 --enable-optimize --disable-debug $EXTRA_OPTIONS ## does not have tests
