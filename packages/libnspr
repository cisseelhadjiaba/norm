#!/bin/bash

fetch_source https://ftp.mozilla.org/pub/nspr/releases/v4.14/src/nspr-4.14.tar.gz 4c9ffda940882229104090f8fc8539f6412e1ff7

EXTRA_OPTIONS=
[[ $HOSTTYPE == *64* ]] && EXTRA_OPTIONS=--enable-64bit

do_unpack
SRCDIR=$SRCDIR/nspr
do_compile_notest --enable-pthreads --enable-ipv6 --enable-optimize --disable-debug $EXTRA_OPTIONS ## does not have tests
