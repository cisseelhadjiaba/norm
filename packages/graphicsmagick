#!/bin/bash

depends_on freetype
depends_on libxml
depends_on glib
depends_on libpcre
depends_on libpng
depends_on zlib
depends_on xz # for liblzma

fetch_source https://downloads.sourceforge.net/graphicsmagick/graphicsmagick/1.3.28/GraphicsMagick-1.3.28.tar.xz f72a8f3f6de27990612ee3e713b2fece1ad898ac

do_unpack_compile
