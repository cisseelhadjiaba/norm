#!/bin/bash

depends_on libselinux ## opportunistic linking
depends_on libgmp ## opportunistic linking
[[ $OSTYPE == *linux* ]] && depends_on libacl ## opportunistic linking
[[ $OSTYPE == *linux* ]] && depends_on libcap ## opportunistic linking

fetch_source https://ftpmirror.gnu.org/coreutils/coreutils-8.29.tar.xz 63d522f430f0f9fceca71e4b52b414999531915c

do_unpack_compile
