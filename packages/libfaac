#!/bin/bash

depends_on autoconf automake libtool

fetch_source https://downloads.sourceforge.net/project/faac/faac-src/faac-1.29/faac-1.29.7.8.tar.gz 4b1dc86f65820c422299bd18edba63d8930382ac

unset EXTRAFLAGS
cc_addflags EXTRAFLAGS -Wno-narrowing

do_unpack
do_autoreconf
CPPFLAGS="$CPPFLAGS $EXTRAFLAGS" \
do_compile --disable-debug
