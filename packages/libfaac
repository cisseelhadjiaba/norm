#!/bin/bash

depends_on autoconf automake libtool

fetch_source https://downloads.sourceforge.net/project/faac/faac-src/faac-1.29/faac-1.29.tar.gz 2383d45160e325a2fc4d601f20a1bf01d6c1b751

unset EXTRAFLAGS
cc_addflags EXTRAFLAGS -Wno-narrowing

do_unpack
do_autoreconf
CPPFLAGS="$CPPFLAGS $EXTRAFLAGS" \
do_compile --disable-debug
