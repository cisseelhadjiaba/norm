#!/bin/bash

depends_on autoconf automake libtool

fetch_source https://downloads.sourceforge.net/project/faac/faac-src/faac-1.29/faac-1.29.7.7.tar.gz 9acf4dd56deea9f17ddcb952e7f11515a67b934e

unset EXTRAFLAGS
cc_addflags EXTRAFLAGS -Wno-narrowing

do_unpack
do_autoreconf
CPPFLAGS="$CPPFLAGS $EXTRAFLAGS" \
do_compile --disable-debug
