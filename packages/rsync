#!/bin/bash

depends_on zlib
[[ $OSTYPE == *linux* ]] && depends_on libattr
[[ $OSTYPE == *linux* ]] && depends_on libacl
depends_on libpopt

fetch_source https://download.samba.org/pub/rsync/src/rsync-3.1.3.tar.gz 82e7829c0b3cefbd33c233005341e2073c425629

do_undebian_compile --without-included-zlib ## security patches break tests
