#!/bin/bash

fetch_source http://http.debian.net/debian/pool/main/u/unzip/unzip_6.0.orig.tar.gz abf7de8a4018a983590ed6f5cbd990d4740f8a22
fetch_debian http://http.debian.net/debian/pool/main/u/unzip/unzip_6.0-21.debian.tar.xz c7401c4282cbf39f4ad3a869400dcf8454a16f4b

do_undebian
pushd_src
do_make -f unix/Makefile LF2="$LDFLAGS" CF="$CFLAGS $CPPFLAGS -I." unzips
do_install -f unix/Makefile prefix=$PREFIX
popd
